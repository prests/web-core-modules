{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "lunar-react-router",
  "description": "React Router demo using Lunar",
  "repository": {
    "type": "git",
    "url": "https://github.com/lunar-js/lunar.git",
    "directory": "demos/lunar-react-router"
  },
  "homepage": "https://github.com/lunar-js/lunar/tree/main/demos/lunar-react-router#readme",
  "private": true,
  "type": "module",
  "scripts": {
    "____ RUN COMMANDS ____": "",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "____ BUILD COMMANDS ____": "",
    "build": "react-router build",
    "____ VALIDATION COMMANDS ____": "",
    "lint": "eslint --max-warnings=0 .",
    "lint:fix": "pnpm run lint --fix",
    "typecheck": "react-router typegen && tsc -p ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "@lunar-js/lunar": "workspace:^",
    "@react-router/node": "^7.9.2",
    "@react-router/serve": "^7.9.2",
    "isbot": "^5.1.31",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-router": "^7.9.2"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.2",
    "@vanilla-extract/css": "^1.16.1",
    "@vanilla-extract/recipes": "^0.5.7",
    "@vanilla-extract/sprinkles": "^1.6.5"
  },
  "license": "MIT"
}
